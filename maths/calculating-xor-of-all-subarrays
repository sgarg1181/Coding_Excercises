#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    if(n%2==0){
	        cout<<0<<endl;
	    }
	    else{
            int c=0;
	       for(int i=0;i<n;i+=2){
	        c^=a[i];
	     }
	     cout<<c<<endl;
	    }
	}
	return 0;
}